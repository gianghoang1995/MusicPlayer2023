<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PlayerViewSmallDetail">
        <attr name="thumbnail" format="reference" />
        <attr name="hideControl" format="reference|boolean" />
    </declare-styleable>

    <declare-styleable name="SwipeViewPager">
        <attr name="swipeEnable" format="reference|boolean" />
    </declare-styleable>

    <declare-styleable name="TitleBarEqualizer">
        <attr name="listTitle" format="reference" />
        <attr name="isTop" format="boolean" />
    </declare-styleable>


    <declare-styleable name="MarkerView">
        <attr name="setRotation" format="integer"/>
    </declare-styleable>

    <declare-styleable name="Croller">
        <attr name="enabled" format="boolean" />
        <attr name="start_progress" format="integer" />
        <attr name="label" format="string" />
        <attr name="back_circle_color" format="color" />
        <attr name="back_circle_disable_color" format="color" />
        <attr name="main_circle_color" format="color" />
        <attr name="main_circle_disable_color" format="color" />
        <attr name="indicator_color" format="color" />
        <attr name="indicator_disable_color" format="color" />
        <attr name="progress_primary_color" format="color" />
        <attr name="progress_primary_disable_color" format="color" />
        <attr name="progress_secondary_color" format="color" />
        <attr name="progress_secondary_disable_color" format="color" />
        <attr name="label_size" format="dimension" />
        <attr name="label_color" format="color" />
        <attr name="label_font" format="string" />
        <attr name="label_style" format="enum" >
            <enum name="normal" value="0"/>
            <enum name="bold" value="1"/>
            <enum name="italic" value="2"/>
            <enum name="bold_italic" value="3"/>
        </attr>
        <attr name="label_disabled_color" format="color" />
        <attr name="indicator_width" format="float" />
        <attr name="is_continuous" format="boolean" />
        <attr name="progress_primary_circle_size" format="float" />
        <attr name="progress_secondary_circle_size" format="float" />
        <attr name="progress_primary_stroke_width" format="float" />
        <attr name="progress_secondary_stroke_width" format="float" />
        <attr name="sweep_angle" format="integer" />
        <attr name="start_offset" format="integer" />
        <attr name="max" format="integer" />
        <attr name="min" format="integer" />
        <attr name="main_circle_radius" format="float" />
        <attr name="back_circle_radius" format="float" />
        <attr name="progress_radius" format="float" />
        <attr name="anticlockwise" format="boolean" />
    </declare-styleable>

    <declare-styleable name="VerticalSeekBar">
        <attr name="seekBarRotation">
            <!-- Clock wise - 90 deg; top = min, bottom = max -->
            <enum name="CW90" value="90" />
            <!-- Clock wise - 270 deg; bottom = min, top = max -->
            <enum name="CW270" value="270" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AlignedTextView">
        <attr name="alignment" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CountDownClock">
        <attr name="resetSymbol" format="string"/>
        <attr name="digitDividerColor" format="color"/>
        <attr name="digitTopDrawable" format="integer"/>
        <attr name="digitBottomDrawable" format="integer"/>
        <attr name="digitSplitterColor" format="color"/>
        <attr name="digitTextColor" format="color"/>
        <attr name="halfDigitHeight" format="dimension"/>
        <attr name="digitWidth" format="dimension"/>
        <attr name="digitTextSize" format="dimension"/>
        <attr name="digitPadding" format="dimension"/>
        <attr name="splitterPadding" format="dimension"/>
        <attr name="animationDuration" format="integer"/>
        <attr name="almostFinishedCallbackTimeInSeconds" format="integer"/>
        <attr name="countdownTickInterval" format="integer"/>
    </declare-styleable>

    <declare-styleable name="AvatarImageBehavior">
        <attr name="finalXPosition" format="dimension"/>
        <attr name="finalYPosition" format="dimension"/>
        <attr name="finalHeight" format="dimension"/>
        <attr name="finalToolbarHeight" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
    </declare-styleable>


</resources>